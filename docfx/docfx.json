{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template":["default"],
    "globalMetadata": {
      "_appName": "OSLoader Documentation",
      "_appTitle": "OSLoader Documentation",
      "_enableSearch": true,
      "pdf": true
    }
  }
}